<h1>All the characters from <%= @movie.name %></h1>

<%= link_to "All the movies", movies_path %>

<% @characters.each do |character| %>
  <div class="character">
    <h2>Name: <%= character.name %></h2>
    <h2>Quote: <%= character.quote %></h2>
    <h2>Movie: <%= character.movie.name %></h2>
  </div>
<% end %>
